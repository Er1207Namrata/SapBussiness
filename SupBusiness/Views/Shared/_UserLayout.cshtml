<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/assets/images/logo/favicon.png">
    <title>Dashboard</title>
    <script src="~/AssociateAssests/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap Core CSS -->
    <link href="~/AssociateAssests/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/AssociateAssests/plugins/datatables/media/css/dataTables.bootstrap4.css" rel="stylesheet">
    <!-- chartist CSS -->
    <link href="~/AssociateAssests/plugins/chartist-js/dist/chartist.min.css" rel="stylesheet">
    <link href="~/AssociateAssests/plugins/chartist-js/dist/chartist-init.css" rel="stylesheet">
    <link href="~/AssociateAssests/plugins/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.css" rel="stylesheet">
    <!--This page css - Morris CSS -->
    <link href="~/AssociateAssests/plugins/c3-master/c3.min.css" rel="stylesheet">
    <link href="~/AssociateAssests/plugins/bootstrap-material-datetimepicker/css/bootstrap-material-datetimepicker.css" rel="stylesheet">
    <!-- Page plugins css -->
    <link href="~/AssociateAssests/plugins/clockpicker/dist/jquery-clockpicker.min.css" rel="stylesheet">
    <!-- Color picker plugins css -->
    <link href="~/AssociateAssests/plugins/jquery-asColorPicker-master/dist/css/asColorPicker.css" rel="stylesheet">
    <!-- Date picker plugins css -->
    <link href="~/AssociateAssests/plugins/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet" type="text/css" />
    <!-- Daterange picker plugins css -->
    <link href="~/AssociateAssests/plugins/timepicker/bootstrap-timepicker.min.css" rel="stylesheet">
    <link href="~/AssociateAssests/plugins/daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/AssociateAssests/css/style.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i%7CQuicksand:300,400,500,700" rel="stylesheet">

    <!-- You can change the theme colors from here -->
    <link href="~/AssociateAssests/css/colors/blue.css" id="theme" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style>
        .privacy {
            text-align: justify;
            font-family: Arial, Helvetica, sans-serif;
            font-size: 13px;
            line-height: 23px;
            height: 500px;
            overflow-x: auto;
            padding-right: 20px;
        }

            .privacy h3 {
                font-size: 16px;
                margin: 0px 0 0px;
                padding: 10px 0 5px;
            }

            .privacy p {
                padding: 0px 0 10px;
                margin: 0px;
            }

            .privacy h2 {
                padding: 0px;
                margin: 0px;
                font-size: 18px;
                text-align: center;
                padding-bottom: 20px;
            }
    </style>
</head>
<body class="fix-header fix-sidebar card-no-border">
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper">

        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        @Html.Partial("_UserHeader")
        <!-- ============================================================== -->
        <!-- End Topbar header -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        @Html.Partial("_UserSidebar")
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- Page wrapper  -->
                <!-- ============================================================== -->
                @RenderBody()
                <!-- ============================================================== -->
                <!-- End Page wrapper  -->
                <!-- ============================================================== -->
            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- footer -->
            <!-- ============================================================== -->
            <footer class="footer">@DateTime.Now.Year © All rights reserved by Sup Bussiness</footer>
            <!-- ============================================================== -->
            <!-- End footer -->
            <!-- ============================================================== -->

        </div>
        <div>
            <style>
                #dvLoader {
                    position: fixed;
                    left: 0;
                    top: 0;
                    bottom: 0;
                    right: 0;
                    background: #000;
                    opacity: 0.7;
                    filter: alpha(opacity=50);
                    display: none;
                    z-index: 99999;
                }

                #loading {
                    position: fixed;
                    top: 50%;
                    left: 50%;
                    z-index: 99999;
                    transform: translate(-50%, -50%);
                    width: 100px;
                }
            </style>
            <div id="dvLoader">
                <img src="~/Images/LoaderTwo.gif" alt="Loading..." id="loading" />

            </div>

        </div>

    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/AssociateAssests/plugins/popper/popper.min.js"></script>
    <script src="~/AssociateAssests/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    @*<script src="~/AssociateAssests/js/jquery.slimscroll.js"></script>*@
    <!--Wave Effects -->
    <script src="~/AssociateAssests/js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="~/AssociateAssests/js/sidebarmenu.js"></script>
    <!--stickey kit -->
    <script src="~/AssociateAssests/plugins/sticky-kit-master/dist/sticky-kit.min.js"></script>
    <script src="~/AssociateAssests/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!--Custom JavaScript -->
    <script src="~/AssociateAssests/js/custom.min.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->
    <!-- chartist chart -->
    <script src="~/AssociateAssests/plugins/chartist-js/dist/chartist.min.js"></script>
    <script src="~/AssociateAssests/plugins/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.min.js"></script>
    <!--c3 JavaScript -->
    <script src="~/AssociateAssests/plugins/d3/d3.min.js"></script>
    <script src="~/AssociateAssests/plugins/c3-master/c3.min.js"></script>
    <!-- Chart JS -->
    <script src="~/AssociateAssests/js/dashboard6.js"></script>
    <script src="~/AssociateAssests/plugins/moment/moment.js"></script>
    <script src="~/AssociateAssests/plugins/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js"></script>
    <!-- Clock Plugin JavaScript -->
    <script src="~/AssociateAssests/plugins/clockpicker/dist/jquery-clockpicker.min.js"></script>

    <!-- Chart JS -->
    <script src="~/AssociateAssests/plugins/echarts/echarts-all.js"></script>
    <script src="~/AssociateAssests/plugins/echarts/echarts-init.js"></script>
    <!-- ============================================================== -->
    <!-- Color Picker Plugin JavaScript -->
    <script src="~/AssociateAssests/plugins/jquery-asColor/dist/jquery-asColor.js"></script>
    <script src="~/AssociateAssests/plugins/jquery-asGradient/dist/jquery-asGradient.js"></script>
    <script src="~/AssociateAssests/plugins/jquery-asColorPicker-master/dist/jquery-asColorPicker.min.js"></script>
    <!-- Date Picker Plugin JavaScript -->
    <script src="~/AssociateAssests/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <!-- Date range Plugin JavaScript -->
    <script src="~/AssociateAssests/plugins/timepicker/bootstrap-timepicker.min.js"></script>
    <script src="~/AssociateAssests/plugins/daterangepicker/daterangepicker.js"></script>
    <script src="~/AssociateAssests/plugins/moment/moment.js"></script>
    <!-- Sweet Alert -->
    <script src="~/assets/js/plugin/sweetalert/sweetalert.min.js"></script>
    <script>
        jQuery('.mdate').datepicker({
            autoclose: true,
            todayHighlight: true,
            format: 'dd/mm/yyyy'
        });
    </script>
    <!-- ============================================================== -->
    <!-- Style switcher -->
    <!-- ============================================================== -->
    <script src="~/AssociateAssests/plugins/styleswitcher/jQuery.style.switcher.js"></script>
    @RenderSection("scripts", false)
    <script>
        $(document).ajaxStart(function () {
            $("#dvLoader").show();
        });
        $(document).ajaxComplete(function (event, request, settings) {
            $("#dvLoader").hide();
        });

        $(document).ajaxError(function (event, request, settings) {
            $("#dvLoader").hide();
        });
        document.addEventListener("contextmenu", function (e) {
            e.preventDefault();
        }, false);
        document.addEventListener("contextmenu", function (e) {
            e.preventDefault();
        }, false);
    </script>
    
</body>

</html>
