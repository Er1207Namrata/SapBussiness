    <script>
        function fSaveLeadDetails() {
            var firstname = $('#FullName').val();
            var EmailId = $('#EmailId').val();
            var PhoneNo = $('#PhoneNo').val();
           // var DesignationId = $('#Pk_DesignationId').val();
            var txtAddress = $('#txtAddress').val();
            $.ajax({
                url: '/Home/SaveLeadRegistration',
                data: { firstname: firstname, EmailId: EmailId, PhoneNo: PhoneNo,txtAddress:txtAddress },
                type: 'POST',
                success: function (response) {
                    if (response != null) {
                        console.log(response);
                        if (response != null) {
                            $('#txtLeadId').text(response.msg)
                            
                        }
                        else {
                            $('#txtLeadId').text('Somthing went wrong')
                            
                        }
                    }
                },
                Error: function () {

                }
            });
        }
    </script>

