@{
    ViewData["Title"] = "LeadRegistrationList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="row page-titles">
    <div class="col-md-12 col-12 align-self-center d-flex justify-content-between">
        <h4 class="page-title mb-0">Lead  List</h4>



    </div>
</div>

<div class="content">
    <div class="page-inner">
        @*<div class="page-header">
        <h4 class="page-title">Sector Master List</h4>
        @if (PermissionManager.IsActionPermit("Sector Master", ViewOptions.FormSave))
        {
        <a href="/Master/SectorMaster" class="btn btn-primary btn-round ml-auto" style="flood-color:white;"><i class="fa fa-plus"></i>Add Sector</a>
        }
        </div>*@
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                    </div>
                    <div class="card-body">
                        <table class="table table-hover">
                            <thead>

                                <tr class="hdr">
                                    <th>S. No.</th>
                                    <th>LoginId </th>
                                    <th>Password </th>
                                    <th>Name </th>
                                    <th>Designation </th>
                                    <th>Email Id </th>
                                    <th>Mobile No  </th>
                                    <th>Address  </th>
                                    <th>Create Date</th>

                                    <th align="left" scope="col">Action</th>

                                </tr>

                            </thead>

                            <tbody>

                                @if (Model != null && Model.dtDetails != null && Model.dtDetails.Rows.Count > 0)
                                {
                                    int s = 0;
                                    for (int i = 0; i < Model.dtDetails.Rows.Count; i++)
                                    {
                                        <tr>
                                            <td>@(s + 1)</td>
                                            <td>@Model.dtDetails.Rows[i]["LoginId"].ToString()</td>
                                            <td>@Model.dtDetails.Rows[i]["Password"].ToString()</td>
                                            <td>@Model.dtDetails.Rows[i]["LeadName"].ToString()</td>
                                            <td>@Model.dtDetails.Rows[i]["Designation"].ToString()</td>
                                            <td>@Model.dtDetails.Rows[i]["EmailId"].ToString()</td>
                                            <td>@Model.dtDetails.Rows[i]["MobileNo"].ToString()</td>
                                            <td>@Model.dtDetails.Rows[i]["Address"].ToString()</td>
                                            <td>@Model.dtDetails.Rows[i]["AddedOn"].ToString()</td>
                                            <td>
                                                <button type="button" class="btn btn-primary text-white"><a href="#" title="Edit">Edit</a></button>
                                                <a href="#" class="text-danger" title="Delete"><i class="fa fa-trash"></i></a>
                                            </td>
                                        </tr>
                                        s++;
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="9"> No Record found !!</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>


</div>



